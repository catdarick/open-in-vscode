{
    "manifest_version": 2,
    "name": "Open in VSCode (BitBucket)",
    "version": "1.0.0",
    "description": "Open BitBucket links in VSCode",
    "page_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "Open in VSCode (BitBucket)"
    },
    "author": "Domenico Gemoli (reworked for BitBucket by Alexey Kurdyukov)",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "background": {
        "scripts": [
            "contextmenu.js"
        ]
    },
    "options_page": "options.html",
    "homepage_url": "https://github.com/catdarick/open-in-vscode",
    "offline_enabled": true,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
